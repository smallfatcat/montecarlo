# Monorepo Environment Template
# Copy to .env or .env.local and set values

# Node.js
NODE_ENV=development

# Frontend (Vite-exposed)
VITE_WS_URL=ws://127.0.0.1:8080
VITE_CONVEX_URL=http://127.0.0.1:3210

# Server
HOST=127.0.0.1
PORT=8080
FRONTEND_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Convex / Backend-only
CONVEX_INGEST_SECRET=replace-me
INSTANCE_SECRET=replace-me
